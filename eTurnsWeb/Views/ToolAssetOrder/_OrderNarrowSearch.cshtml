    function FillNarrowSearchData() {
        $.ajax({
            url: '@Url.Content("~/ToolAssetOrder/GetNarrowSearchData")',
            data: { 'IsDeleted': _IsDeleted, 'IsArchived': _IsArchived, 'OrderStatus': GetCurrentSelectedTab() },
