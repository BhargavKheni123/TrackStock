                    if (response.Data.UDF4List != null) {
                        $('#liOrdUDF4').css('display', '');
                        BindDropDownList(response.Data.UDF4List, 'OrderUDF4', 'OrderUDF4Collapse', '@eTurns.DTO.ResToolAssetOrder.UDF4');
