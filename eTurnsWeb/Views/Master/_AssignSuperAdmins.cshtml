@model List<eTurns.DTO.EnterpriseSuperAdmin>
@{



}

<table class="display" id="dtepusers">
    <thead>
        <tr>
            <th>
                @ResUserMaster.IsEPSuperAdmin
            </th>
            <th>
                @ResUserMaster.UserName
            </th>
            <th>
                @ResUserMaster.Role
            </th>
            <th>
                @ResUserMaster.EmailAddress
            </th>
            <th>
                @ResUserMaster.IsDefault
            </th>
            <th>
                @ResUserMaster.MarkDeleted
            </th>


        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>
                    @Html.CheckBoxFor(t => Model[i].IsEPSuperAdmin)

                </td>
                <td>
                    @Html.DisplayFor(t => Model[i].UserName)

                </td>
                <td>
                    @Html.DisplayFor(t => Model[i].RoleName)
                </td>
                <td>
                    @Html.DisplayFor(t => Model[i].Email)
                </td>
                <td>
                    @Html.CheckBoxFor(t => Model[i].IsDefault)
                </td>
                <td>
                    @Html.CheckBoxFor(t => Model[i].MarkDeleted)
                </td>
            </tr>

        }
    </tbody>
   
</table>
<input type="button" name="btnAddNewUser" id="btnAddNewUser" value="@ResCommon.AddNew" />
<script type="text/javascript">
    $(document).ready(function () {
        var dtepusers = $('#dtepusers').dataTable();
        $("#btnAddNewUser").click(function () {
            var LastTR = $("#dtepusers").find("tbody").find("TR:last");
            $("#dtepusers").find("tbody").append(LastTR);            
            $('#dtepusers').dataTable();
        });
    });


</script>