<script>
    function openUserDetail()
    {
        var UserName = $("input#UserNameList").val();
        var strUrl = '@Url.Action("UserLoginwithDetail")';
        //strUrl = strUrl + "?UserName='" + UserName+"'";
        //$('#UserLoginModelPS').data("url", strUrl).dialog('open');
        $.ajax({
            type: "POST",
            url: strUrl,
            data: "{UserName: '" + UserName + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: 'html',
            success: function (r) {
                if ($('#UserLoginModelPS').dialog('isOpen') && window.location.href.toLowerCase().indexOf('enterpriselist') >= 0) {
                    $("#UserLoginModelPS").html(r);
                }
                else {
                    $("#UserLoginModelPS").html(r);
                    $("#UserLoginModelPS").dialog("open");
                 
                }
                $('#DivLoading').hide();
            }
        });
    }
    $(document).ready(function () {

        $("#UserLoginModelPS").dialog({
            autoOpen: false,
            modal: true,
            draggable: true,
            resizable: true,
            width: '82%',
            height: 650,
            title: "@ResEnterprise.TitleLoginUserDetails",
            open: function () {
                $('#DivLoading').show();
                $("#UserLoginModelPS").load($(this).data("url"));
            },
            close: function (response) {
                //CallThisFunctionFromModel('success');

            }
        });
    });
</script>
