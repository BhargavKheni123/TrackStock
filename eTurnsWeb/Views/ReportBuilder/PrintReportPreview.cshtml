@{
    ViewBag.Title = "PrintReportPreview";
    Layout = "~/Views/Shared/_PopupMaster.cshtml";
}

<embed type="application/pdf" src="@ViewBag.URLToPdf" id="pdfDocument" />
@*<iframe src="@ViewBag.URLToPdf" id="pdfDocument" height="800" width="800" />*@

<script src="@Url.Content("~/Content/OpenAccess/jquery-1.7.1.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var h = $(window).height() - 40;
        var w = $(window).width() - 20;
        $("#pdfDocument").height(h).width(w);
    });
</script>
