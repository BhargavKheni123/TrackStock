        <th>
            <em>* </em>
            @ResUnitMaster.Unit
        </th>
    }
    @if (arrcolumns.Contains("description"))
    {
        <th>
            @ResUnitMaster.Description
        </th>
    }
    @if (arrcolumns.Contains("odometer"))
