        <th>
            @ResUnitMaster.SerialNo
        </th>
    }
    @if (arrcolumns.Contains("year"))
    {
        <th>
            @ResUnitMaster.Year@ResUnitMaster.Year
        </th>
    }
    @if (arrcolumns.Contains("make"))
    {
        <th>
            @ResUnitMaster.Make
        </th>
    }
    @if (arrcolumns.Contains("model"))
