        ? entpriseIdsForReleaseNo : string.Empty; // "20200,10052";
    //                                              //List<long> EntListToShowReleaseNo = EnterpriseListToShowReleaseNo.Split(',').Select(long.Parse).ToList();
    long convertedInt;
    var EntListToShowReleaseNo = EnterpriseListToShowReleaseNo.Split(new char[] { ',' }, StringSplitOptions.RemoveEmptyEntries)
