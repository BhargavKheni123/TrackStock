    function callPrint1(DataTableName) {
        var oConfig = {
            "sInfo": "<h6>Print view</h6><p>Please use your browser's print function to " + "print this table. Press escape when finished.",
            "sMessage": null,
            "bShowAll": false,
            "sToolTip": "View print view",
            "sButtonClass": "DTTT_button_print",
            "sButtonText": "Print"
        };
        if (typeof (TableTools) != undefined && typeof (TableTools) != 'undefined')
            TableTools.fnGetInstance(DataTableName).fnPrint(true, oConfig);
    }
    function ChildGridReloadAfterReorder() {
        OrderListFlag = false;
        $('#DivLoading').show();
        $("#divWOLineItemHistory").empty();
        $.get('WorkOrderHistoryView', { ID: HostoryId }, function (data) {
            $("#divWOLineItemHistory").html(data);
        });
    }

    function ChildGridColumnAndTableRebinding() {
        oTableGlobalTABLE = $("#" + "WOItemsTableHistory" + '@(Model.ID)').dataTable();
        objColumns = WOChildGridTableColumns;
    }
    var GetColumnIndex = function (ColumnName) {
        if (OrderListFlag == true) {
            if (typeof (WOChildGridTableColumns) != "undefined") {
                return WOChildGridTableColumns[ColumnName];
            }
        }
        else
        {
            if (typeof (objColumns) != "undefined") {
                return objColumns[ColumnName];
            }
        }
    };
    $('#ColumnOrderSetupChildGrid').on("click", function () {
        OrderListFlag = true;
        $("#ColumnSortableModalChildGrid").dialog("open");
