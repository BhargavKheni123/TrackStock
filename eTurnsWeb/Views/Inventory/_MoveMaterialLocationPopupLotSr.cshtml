            function IsLotSerialExistsInCurrentLoaded(strIds, SerialLot, BinNumber) {
                if (SerialLot.trim() == '')
                    return true;

                if (strIds.trim() == '')
                    return false

                if (typeof BinNumber != "undefined"
                    && BinNumber != undefined
                    && BinNumber != null
                    && BinNumber.trim() != '') {
                    var ArrIds = strIds.split(',');
                    var i = 0;
                    for (i = 0; i < ArrIds.length; i++) {
                        if (ArrIds[i].split('&_&')[0].toLowerCase() == SerialLot.toLowerCase()
                            && ArrIds[i].split('&_&')[1] == BinNumber) {
                            return true;
                        }
                    }
                }
                else {
                    var ArrIds = strIds.split(',');
                    var i = 0;
                    for (i = 0; i < ArrIds.length; i++) {
                        if (ArrIds[i].split('&_&')[0].toLowerCase() == SerialLot.toLowerCase()) {
                            return true;
                        }
                    }
                }
                return false;
            }


